.uni-swiper-tab {
  border-bottom: 1rpx solid #EEEEEE;
  text-align: center;
}
.swiper-tab-list {
  color: #969696;
  font-weight: bold;
  display: inline-block;
  margin-left: 30rpx;
}
.uni-tab-bar .active {
  color: #343434;
}
.active .swiper-tab-line {
  border-bottom: 9rpx solid #FEDE33;
  width: 70rpx;
  margin: auto;
  border-top: 6rpx solid #FEDE33;
  border-radius: 20rpx;
}

