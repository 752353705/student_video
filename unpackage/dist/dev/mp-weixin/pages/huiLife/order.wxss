.mall {
  padding-bottom: 60rpx;
}
.mall .head_box {
  background-color: #ffffff;
  position: -webkit-sticky;
  position: sticky;
  top: 0rpx;
  z-index: 10;
}
.mall .head_box .classification {
  background-color: #ffffff;
  width: 100%;
  padding-bottom: 12rpx;
}
.mall .head_box .classification .classification_item {
  color: #6d7278;
  box-sizing: border-box;
  position: relative;
  border-left: 1rpx solid #e7ecf1;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  padding: 0rpx 20rpx;
  width: 150rpx;
}
.mall .head_box .classification .classification_item.act {
  color: #358d4d;
}
.mall .head_box .classification .classification_item .fri_img {
  width: 4rpx;
  height: 28rpx;
}
.mall .head_box .classification .classification_item .btm_border {
  width: 78rpx;
  height: 5rpx;
  background-color: #358d4d;
  margin-top: 8rpx;
  position: absolute;
  bottom: -11rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.mall .head_box .classification .classification_item:nth-child(1) {
  border-right: 1rpx solid #e7ecf1;
  padding-right: 20rpx;
}
.mall .head_box .classification .classification_item:nth-child(1) .fri_img {
  margin-left: 20rpx;
}
.mall .head_box .classification .classification_item:nth-child(1) .btm_border {
  width: 28rpx;
}
.mall .head_box .classification .classification_item:nth-child(2) {
  border: none;
}
.mall .main {
  padding: 0rpx 20rpx 0rpx;
}
.mall .main .item {
  margin-top: 14rpx;
  border-radius: 10rpx;
  background-color: #FFFFFF;
  padding: 24rpx 14rpx 14rpx 22rpx;
}
.mall .main .item .item_head .item_head_le {
  width: 174rpx;
  height: 174rpx;
  background-color: #F1F8FF;
  border-radius: 4rpx;
  margin-right: 26rpx;
}
.mall .main .item .item_head .item_head_ri {
