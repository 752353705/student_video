.liveStreaming .navigationBar {
  padding: 78rpx 0rpx 14rpx 20rpx;
  position: relative;
  background-color: #FFFFFF;
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 10;
}
.liveStreaming .navigationBar .classify {
  position: relative;
}
.liveStreaming .navigationBar .classify .back {
  width: 33rpx;
  height: 34rpx;
  position: absolute;
  left: 0rpx;
  top: 8rpx;
}
.liveStreaming .navigationBar .classify .line {
  width: 47rpx;
  height: 47rpx;
  position: absolute;
  left: 104rpx;
  top: 2rpx;
}
.liveStreaming .navigationBar .classify .item {
  position: relative;
}
.liveStreaming .navigationBar .classify .item .act {
  color: #358D4D;
}
.liveStreaming .navigationBar .classify .item .act_border {
  position: absolute;
  width: 40rpx;
  height: 4rpx;
  background-color: #358D4D;
  bottom: -5rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.liveStreaming .navigationBar .classify .item:nth-child(4) {
  margin-left: 64rpx;
}
.liveStreaming .navigationBar .classify .item:nth-child(5) {
  margin-left: 64rpx;
}
