.pop .imgBox {
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.pop .imgBox .img {
  width: 62.5rpx;
  height: 62.5rpx;
  margin-top: 20rpx;
}
.find {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  box-sizing: border-box;
  padding: 33rpx 11.5rpx 0;
  color: white;
}
.find .search {
  color: black;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  margin-bottom: 19rpx;
}
.find .search .left {
  background-color: #f5f5f5;
  border-radius: 20rpx;
  width: 85%;
}
.find .search .left input {
  width: 70%;
  height: 70rpx;
  display: inline-block;
}
.find .search .left image {
  width: 40rpx;
  height: 40rpx ;
  float: left;
  -webkit-transform: translateY(45%);
          transform: translateY(45%);
  margin-right: 12rpx;
  margin-left: 10rpx;
}
.find .search .searchBtn {
  color: black;
}
.find .his_box {
  position: absolute;
  z-index: 10;
  width: 100%;
  background-color: gray;
  opacity: 0.8;
  border-radius: 0 0 50rpx 50rpx;
  color: black;
}
.find .his_box .title {
  font-size: 29rpx;
  margin-top: 20rpx;
}
.find .his_box .his_list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  margin-bottom: 20rpx;
}
.find .his_box .his_list .his_item {
  padding: 9rpx;
  border-radius: 20rpx;
  border: 1rpx solid white;
  margin-left: 19rpx;
  margin-top: 34rpx;
  margin-right: 19rpx;
}

